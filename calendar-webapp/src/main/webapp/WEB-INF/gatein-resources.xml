  <!-- Jquery placeholder plugin -->
  <module>
    <name>placeholder</name>
    <script>
      <adapter>
        (function($) {
        var _$ = window.jQuery; window.jQuery = $;
        <include>/javascript/jquery-plugins/jquery.placeholder.js</include>
        window.jQuery = _$;
        })($);
      </adapter>
    </script>
    <depends>
      <module>jquery</module>
    </depends>
  </module>

